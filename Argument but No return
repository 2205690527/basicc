// Online C compiler to run C program online
#include <stdio.h>
 areaofcircle(int r)
 {
    float area;
    area=3.14*r*r;
    printf("area of circle %f",area);
 }
 main() 
 {
    int radius;
    printf("enter radius");
    scanf("%d",&radius);
    areaofcircle(radius);
    printf(" thank you");
 }
