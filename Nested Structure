// Online C compiler to run C program online
#include <stdio.h>
 struct student 
 {
    int age;
    char name[20];
    float percentage;
 } s[4];
 main()
 {
    int i;
    for(i=0;i<4;i++)
    {
        printf("enter age,name,per=");
        scanf("%d%s%f",&s[i].age,s[i].name,&s[i].percentage);
    }   
    for(i=0;i<4;i++)
    {
        printf("details of student \n");
        printf("name is %s\n",s[i].name);
        printf("age is %d\n",s[i].age);
        printf("percentage %f\n",s[i].percentage);
    }
 }
